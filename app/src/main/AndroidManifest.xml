<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pro.devapp.apps.composetable">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeTable">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeTable.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MultiScrollTableActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".MultiScrollTable2Activity"
            android:exported="false">
        </activity>
        <activity
            android:name=".MultiScrollComplexTableActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".MultiScrollComplexTableWithHeadersActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".MultiScrollComplexTableWithHeaders2Activity"
            android:exported="false">
        </activity>
    </application>

</manifest>